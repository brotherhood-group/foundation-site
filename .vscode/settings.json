{
  "deno.enable": true,
  "deno.lint": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "deno.path": "/home/linuxbrew/.linuxbrew/bin/deno",
  "editor.formatOnSave": true,
  "css.validate": false,
  "editor.suggest.showStatusBar": true,
